{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "next lint --fix",
    "db:start": "docker-compose -f docker-compose.dev.yml up -d",
    "db:stop": "docker-compose -f docker-compose.dev.yml down",
    "db:reset": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.dev.yml up -d",
    "db:generate": "cd ../../packages/media-db && npx prisma generate && cd ../company-db && npx prisma generate && cd ../admin-db && npx prisma generate",
    "db:migrate": "cd ../../packages/media-db && npx prisma migrate deploy && cd ../company-db && npx prisma migrate deploy && cd ../admin-db && npx prisma migrate deploy",
    "db:studio": "cd ../../packages/media-db && npx prisma studio",
    "db:seed": "cd ../../packages/media-db && npm run seed",
    "db:reset-with-seed": "yarn db:reset && yarn db:generate && yarn db:migrate && yarn db:seed",
    "setup:manual": "echo セットアップ手順: 1.yarn db:start 2.yarn db:generate 3.yarn db:migrate 4.yarn dev",
    "dev:kill": "wmic process where \"commandline like '%next%start-server%'\" delete 2>nul || echo No Next.js processes found",
    "dev:clean": "yarn dev:kill && timeout /t 3 >nul",
    "dev:safe": "yarn dev:clean && yarn dev",
    "port:check": "netstat -ano | findstr :3000 || echo Port 3000 is free",
    "port:kill": "for /f \"tokens=5\" %a in ('netstat -ano ^| findstr :3000') do wmic process where processid=%a delete 2>nul",
    "env:status": "echo === Environment Status === && yarn port:check && docker ps --filter name=web-dev-database --format \"table {{.Names}}\t{{.Status}}\"",
    "dev:utils": "scripts\\dev-utils.bat",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker:up": "docker-compose -f docker-compose.dev.yml up",
    "docker:up:detached": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:restart": "docker-compose -f docker-compose.dev.yml down && docker-compose -f docker-compose.dev.yml up",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f web",
    "docker:shell": "docker-compose -f docker-compose.dev.yml exec web sh",
    "docker:clean": "docker-compose -f docker-compose.dev.yml down -v && docker system prune -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/lib-storage": "^3.525.1",
    "@hookform/resolvers": "^3.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "@trpc/client": "^10.34.0",
    "@types/node": "20.11.19",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "framer-motion": "^11.13.1",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.439.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.6",
    "next": "15.1.6",
    "next-auth": "^4.22.1",
    "next-intl": "^3.26.3",
    "postcss": "8.4.24",
    "prisma": "^6.14.0",
    "puppeteer": "^20.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-infinite-scroller": "^1.2.6",
    "react-markdown": "^9.0.3",
    "react-modal": "^3.16.1",
    "react-select": "^5.7.7",
    "react-spinners": "^0.13.8",
    "react-use": "^17.4.0",
    "sass": "^1.64.2",
    "sharp": "^0.32.6",
    "superjson": "^1.12.4",
    "swiper": "^11.2.6",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.7",
    "tiktok-signature": "^1.9.2",
    "ts-node": "^10.9.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tsconfig/next": "^1.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20",
    "@types/react-modal": "^3.16.3",
    "typescript": "5.9.2"
  }
}
